
public class Exe {
	public static void main(String[] args) {
		In in = new In(args[0]);
		WeightedGraph G = new WeightedGraph(in);
		
		char c = args[1].charAt(0);
		
		int s = c - 'A';
		
		Dijkstra dij = new Dijkstra(G, s);
		double[] distance = dij.distance();
		
		for (int i = 0; i < G.V(); i++) {
			StdOut.printf("%.0f ", d[i]);
		}
		
	}	
}
