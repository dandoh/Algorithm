
public class EulerTour {
	private Graph G;
	private int[] deg;
	private boolean hasTour = true;
	
	
	public EulerTour(Graph G) {
		this.G = G;
		this.deg = new int[G.V()];
		for (int i = 0; i < G.V(); i++) {
			deg[i] = G.degree(i);
		}
		for (int i = 0; i < G.V(); i++) {
			if (deg[i] % 2 != 0)
				hasTour = false;
		}
		
		if (hasTour) {
			
		}
	}
	
	public boolean hasTour() {
		return hasTour;
	}
	
	private void dfs(Graph G, int v) {
		
	}
	
	public Iterable<Integer> tour() {
		assert hasTour();
		
	}
}
