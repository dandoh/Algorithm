
public class WeightedGraph {
	private int V;
	private Bag<Edge>[] adj;
	
	@SuppressWarnings("unchecked")
	public WeightedGraph(int V) {
		this.V = V;
		adj = (Bag<Edge>[]) new Bag[V];
		
		for (int i = 0; i < V; i++) {
			adj[i] = new Bag<Edge>();
		}
	}
	
	public WeightedGraph(In in) {
		this(in.readInt());
		
		int E = in.readInt();
		StdOut.println(E);
		for (int i = 0; i < E; i++) {
			String a = in.readString();
			char temp = a.charAt(0);
			int v = temp - 'A';
			StdOut.println(v);
			String b = in.readString();
			temp = b.charAt(0);
			int w = temp - 'A';
			StdOut.println(w);
			double weight = in.readDouble();
			Edge e = new Edge(v, w, weight);
			addEdge(v, e);
			addEdge(w, e);
		}
	}
	
	public void addEdge(int v, Edge e) {
		adj[v].add(e);
	}
	
	public void print() {
		for (int i = 0; i < V; i++) {
			for (Edge e : adj[i]) {
				StdOut.printf("%.0", e.weight());
			}
		}
	}
}
