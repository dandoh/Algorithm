import java.util.Iterator;


public class Euler {
	private Iterator<Integer>[] adj;
	private boolean hasCycle;
	private Stack<Integer> c;
	private boolean[] onStack;
	
	@SuppressWarnings({ "unused", "unchecked" })
	public Euler(Graph G) {
		hasCycle = true;
		for (int i = 0; i < G.V(); i++) {
			int sum = 0;
			for (int j : G.adj(i)) {
				sum++;
			}
			if (sum % 2 == 1) { 
				hasCycle = false;
				return;
			}
		}
		
		adj = (Iterator<Integer>[]) new Iterator[G.V()];
		for (int i = 0; i < G.V(); i++) {
			adj[i] = G.adj(i).iterator();
		}
		
		c = new Stack<Integer>();
		onStack = new boolean[G.V()];
		dfs(G, 0, 0);
	}
	
	private void dfs(Graph G, int v, int before) {
		onStack[v] = true;
		
		for (int w : G.adj(v)) {
			if (w != before) {
				if (onStack[v]) {
					
				}
			}
		}
		
	}

	public boolean hasCycle() {
		return hasCycle;
	}
	
	public Iterable<Integer> cycle() {
		if (!hasCycle()) 
			throw new UnsupportedOperationException("There is no cycle !");
		return c;
	}
	
	public static void main(String[] args) {
		
	}
}
