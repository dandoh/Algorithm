
public class Bipartite {	
	private Graph G;
	private boolean[] marked;
	private int[] group;
	private int[] parent;
	
	public Bipartite(Graph G) {
		this.G = G;
		marked = new boolean[G.V()];
		for (int i = 0; i < G.V(); i++) 
			if (!marked[i]) 
				dfs(G, i);
	}
	
	public void dfs(Graph G, int s) {
		marked[s] = true;
		
	}
}
