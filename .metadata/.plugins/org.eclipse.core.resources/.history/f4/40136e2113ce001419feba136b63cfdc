
public class EulerTour {
	private Graph G;
	private int[] deg;
	private boolean hasTour = true;
	private boolean[] marked;
	
	
	public EulerTour(Graph G) {
		this.G = G;
		deg = new int[G.V()];
		marked = new boolean[G.V()];
		for (int i = 0; i < G.V(); i++) {
			deg[i] = G.degree(i);
		}
		for (int i = 0; i < G.V(); i++) {
			if (deg[i] % 2 != 0) {
				hasTour = false;
				break;
			}
		}
		
		if (hasTour) {
			dfs(G, 0, 0);
		}
	}
	
	public boolean hasTour() {
		return hasTour;
	}
	
	private void dfs(Graph G, int v, int start) {
		deg[v] -= 2;
		for (int u : G.adj(v)) {
			if (u == start) { 
				return;
			} else {
				dfs(G, u, start);
			}
		}
	}
	
	public Iterable<Integer> tour() {
		assert hasTour();
		
		
	}
}
